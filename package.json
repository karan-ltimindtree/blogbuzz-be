{
  "name": "blogbuzz-be",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "temp": "env-cmd -f .env.local npx ts-node index.ts",
    "local": "env-cmd -f .env.local nodemon",
    "dev": "env-cmd -f .env.dev node ./dist/app.js",
    "build": "npx tsc",
    "lint": "eslint src/**/*.ts",
    "lint:format": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "prisma:migrate": "env-cmd -f .env.local prisma migrate dev --skip-seed",
    "prisma:migrate:dev": "env-cmd -f .env.dev prisma migrate dev --skip-seed",
    "prisma:generate": "env-cmd -f .env.local npx prisma generate",
    "prisma:generate:dev": "env-cmd -f .env.dev npx prisma generate",
    "prisma:studio": "env-cmd -f .env.local npx prisma studio",
    "prisma:studio:dev": "env-cmd -f .env.dev npx prisma studio",
    "prisma:seed": "env-cmd -f .env.local npx ts-node ./prisma/seed.ts",
    "prisma:seed:dev": "env-cmd -f .env.dev npx ts-node ./prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
